<form action="/postchat" method="post" class="modal-content animate">
    <span onclick="document.getElementById('id01').style.display='none'" class="close" title="Close Modal">&times;</span>
    <div id="name-div">
        <input id="username" name="username" autocomplete="off" autofocus placeholder="Enter your Name" />
        <input id="Email" name="Email" />
        <button>Submit</button>
    </div>
    <div id="welcome"></div>
    <ul id="messages"></ul>
    <div id="input-div" style="height: 35px">
        <input id="message" name="message" autocomplete="off" placeholder="Type your message here"/>
        <button>Send</button>
    </div>
</form>
</div>
<script>
    websocket = new WebSocket("ws://localhost:8080/");
    $('form').submit(function() {
        name = $('#username').val() ? $('#username').val() : 'Anonymous';
        $('#name-div').hide();
        $('#welcome').text('Hello ' + name);
        websocket.send(JSON.stringify({
            name: name,
            message: $('#message').val()
        }));
        $('#message').focus();
        $('#message').val('');
        return false;
    });
    websocket.onmessage = function(evt) {
        $('#messages').append($('<li>').html(evt.data));
    };
    websocket.onerror = function(evt) {
        $('#messages').append($('<li>').text('<span style="color: red;">ERROR:</span> ' + evt.data));
    };
    // Get the modal
    var modal = document.getElementById('id01');
    // When the user clicks anywhere outside of the modal, close it
    window.onclick = function(event) {
        if (event.target == modal) {
            modal.style.display = "none";
        }
    }

</script>

